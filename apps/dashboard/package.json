{
  "name": "@pravado/dashboard",
  "version": "1.0.0-rc1",
  "releaseCandidate": true,
  "description": "Pravado v2 Dashboard - Next.js admin interface",
  "private": true,
  "scripts": {
    "dev": "WATCHPACK_POLLING=true next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "check:invariants": "node scripts/check-network-invariant.mjs",
    "check:command-center": "node scripts/check-command-center-kpis.mjs && node scripts/check-entity-map-spec.mjs && node scripts/check-command-center-v1-contract.mjs",
    "check:pr": "node scripts/check-pr-work-surface-contract.mjs",
    "check:pr-v1": "node scripts/check-pr-work-surface-v1.mjs",
    "check:pr-v11": "node scripts/check-pr-work-surface-v11.mjs",
    "check:contracts": "npm run check:command-center && npm run check:pr && npm run check:pr-v1 && npm run check:pr-v11",
    "check:typography": "node scripts/check-ds3-typography.mjs",
    "check:typography:all": "node scripts/check-ds3-typography.mjs --all",
    "check:typography:update": "node scripts/check-ds3-typography.mjs --update",
    "seed:pr": "node scripts/seed-pr-data.mjs --execute",
    "seed:pr:dry": "node scripts/seed-pr-data.mjs",
    "seed:pr:clean": "node scripts/seed-pr-data.mjs --execute --clean",
    "verify:pr": "node scripts/verify-pr-backend.mjs",
    "check:pr-strict-smoke": "node scripts/check-pr-strict-smoke.mjs",
    "check:pr-guardrails": "node scripts/check-pr-guardrails.mjs",
    "test:visual": "npx playwright test tests/visual",
    "test:visual:update": "npx playwright test tests/visual --update-snapshots",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@pravado/feature-flags": "workspace:*",
    "@pravado/types": "workspace:*",
    "@pravado/utils": "workspace:*",
    "@pravado/validators": "workspace:*",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.312.0",
    "next": "^14.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reactflow": "^11.11.4",
    "server-only": "^0.0.1",
    "swr": "^2.3.8",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "msw": "^2.12.7",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}